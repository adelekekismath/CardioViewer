{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "lib": ["dom", "es2017"],
        "types": ["cornerstone-core", "cornerstone-tools", "cornerstone-wado-image-loader"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
}
